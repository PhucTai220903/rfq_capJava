<mvc:View
    controllerName="rfqportal.controller.Guest"
    xmlns:mvc="sap.ui.core.mvc"
    xmlns="sap.m">
    <Page id="guestView_navi" title="Welcome to RFQ Portal NPT">
        <content>
            <VBox class="sapUiMediumMargin" justifyContent="Center" alignItems="Center">
                <Button 
                    text="Go to Admin Dashboard" 
                    press="onNavigateToAdmin" 
                    width="20rem" 
                    type="Emphasized"/>
                <Button 
                    text="Go to Vendor Dashboard" 
                    press="onNavigateToVendor" 
                    width="20rem" 
                    type="Accept"/>
                <Button 
                    text="Go to Buyer Dashboard" 
                    press="onNavigateToBuyer" 
                    width="20rem" 
                    type="Attention"/>
            </VBox>
        </content>
    </Page>
</mvc:View>